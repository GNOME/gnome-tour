#~ msgstr "日付と時刻"

#~ msgid "System Menu"
#~ msgstr "システムメニュー"

#~ msgid "Software"
#~ msgstr "ソフトウェア"

#~ msgid "Tour Completed"
#~ msgstr "ツアー終了"
